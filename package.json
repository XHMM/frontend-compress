{
  "name": "frontend-image-compress",
  "version": "3.0.2",
  "description": "compress image-type file in frontend using canvas",
  "main": "./dist/index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "watch": "parcel watch src/index.ts --global Compress --no-source-maps",
    "build": "parcel build src/index.ts --global Compress --out-file index.min.js --no-source-maps",
    "tsc": "tsc",
    "prepublishOnly": "rimraf dist && npm run build && npm run tsc"
  },
  "keywords": [
    "image-compress",
    "canvas",
    "file-compress",
    "frontend-compress"
  ],
  "author": "xhmm",
  "license": "MIT",
  "repository": "https://github.com/XHMM/frontend-image-compress",
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "typescript": "3.8.3",
    "rimraf": "^3.0.2"
  }
}
