{
  "name": "frontend-image-compress",
  "version": "3.0.0",
  "description": "compress image-type file in frontend using canvas",
  "main": "./dist/frontend-image-compress.js",
  "typings": "./index.d.ts",
  "scripts": {
    "watch": "parcel watch ./src/frontend-image-compress.ts --global Compress --no-source-maps",
    "build": "parcel build ./src/frontend-image-compress.ts --global Compress --out-file frontend-image-compress.min.js --no-source-maps",
    "def": "cd dist && tsc"
  },
  "keywords": [
    "image-compress",
    "canvas",
    "file-compress",
    "frontend-compress"
  ],
  "author": "xhmm",
  "license": "MIT",
  "repository": "https://github.com/XHMM/frontend-image-compress",
  "devDependencies": {
    "parcel-bundler": "^1.12.2",
    "typescript": "3.2.1"
  }
}
